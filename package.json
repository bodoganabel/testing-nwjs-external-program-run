  },
  "homepage": "https://github.com/bodoganabel/testing-nwjs-external-program-run#readme",
  "devDependencies": {
    "nw": "^0.44.1-sdk",
    "nw-builder": "^3.5.7",
    "nwjs-builder-phoenix": "^1.15.0"
  }
}